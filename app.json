{
  "expo": {
    "name": "Agenda Ninho",
    "slug": "agendapsi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hatching-chick.png",
    "userInterfaceStyle": "automatic",
    "scheme": "myapp",
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hatching-chick.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ninho.agenda"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f448f1d2-6573-4064-830f-b1a5d04792b7"
      }
    }
  }
}
